{
    "name": "jrluis/laravel-oracle-logs",
    "description": "Laravel log driver for Oracle Cloud Logs service",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "João Luís",
            "email": "jrluis@jrluis.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/log": "^9.0|^10.0|^11.0|^12.0",
        "hitrov/oci-api-php-request-sign": "^1.2",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jrluis\\LaravelOracleLogs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jrluis\\LaravelOracleLogs\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jrluis\\LaravelOracleLogs\\OracleLogsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
